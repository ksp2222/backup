package chap23_stream.card;

public class CreditCard {
	private String company;
	private String cardName;
	private int limit;
	public CreditCard() {
		
	}
	
	public CreditCard(String company, String cardNamem int limit) {
		this.company = company;
		this.cardName = cardName;
		this.limit = limit;
	}

}
